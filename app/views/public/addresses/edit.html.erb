<h3>配送先編集</h3>

  <%= form_with model: @address do |f| %>
    <%= f.label :post_code, "郵便番号(ハイフンなし)" %>
    <%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %><br>
    <%= f.label :address, "住所" %>
    <%= f.text_field :address, autocomplete: "adderess" %><br>
    <%= f.label :name, "宛名" %>
    <%= f.text_field :name, autocomplete: "name" %><br>
    <%= f.submit '変更内容を保存'%>
  <% end %>

    </div>
  </div>  
</div>